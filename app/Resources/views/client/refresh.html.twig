{% extends 'base.html.twig' %}

{% block body %}
    <h1>Clients list</h1>

    <table class="table table-bordered">
        <thead>
            <tr>             
                <th>Mac</th>
                <th>Selfname</th>
            </tr>
        </thead>
        <tbody>
        {% for client in resulines %}
            <tr>
            
                <td>{{ client.chaddr }}</td>
                <td>{{ client['Host-Name'] }}</td>
               
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
